{
    "table": "cms_backup_media",
    "order": "!date",
    "label": "Backup danych - pliki",
    "disable": [
        "add",
        "remove",
        "backup"
    ],
    "buttons_edit": [
        {
            "label_PL": "Przywróć media",
            "label_EN": "Restore media",
            "icon": "restore",
            "plugin": "cms_backup_media_restore"
        }
    ],
    "buttons_page": [
        {
            "label": "Backup Data",
            "icon": "backup",
            "type": "page",
            "page": "cms_backup?s_page={{get.s_page}}&s_record={{get.s_record}}"
        }
    ],
    "fields": [
        {
            "field": "date",
            "type": "date",
            "cms": {
                "label": "Data",
                "list": "show",
                "edit": false
            }
        },
        {
            "cms": {
                "label": "Sesja",
                "list": "show",
                "edit": false
            },
            "field": "session",
            "type": "select",
            "source": {
                "model": "cms_users_logs_logins"
            }
        },
        {
            "field": "page",
            "cms": {
                "hr": true,
                "label": "Strona",
                "search": "strict",
                "list": "show",
                "edit": false
            }
        },
        {
            "field": "model",
            "cms": {
                "label": "Model",
                "list": "show",
                "edit": false,
                "search": "strict",
                "hr": true
            }
        },
        {
            "field": "record",
            "cms": {
                "label": "Rekord",
                "list": "show",
                "edit": false,
                "search": true
            }
        },
        {
            "field": "field",
            "cms": {
                "label": "Pole",
                "list": "show",
                "edit": false,
                "search": true
            }
        },
        {
            "field": "uid",
            "cms": {
                "label": "uid",
                "edit": false,
                "list": "read",
                "hr": true
            }
        },
        {
            "field": "checksum",
            "cms": {
                "label": "checksum",
                "edit": false
            }
        },
        {
            "field": "extension",
            "cms": {
                "label": "Rozszerzenie",
                "edit": false,
                "list": "read"
            }
        },
        {
            "type": "file",
            "field": "file",
            "settings": {
                "folder": "/public/upload/cms_backup",
                "filename": "%uid%.%extension%"
            },
            "cms": {
                "edit": false,
                "list": {
                    "height": 100
                }
            }
        },
        {
            "field": "path",
            "cms": {
                "label": "Ścieżka źródłowa",
                "edit": false
            }
        }
    ]
}