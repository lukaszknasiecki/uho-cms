{
    "table": "cms_users_logs_logins",
    "disable": [
        "add",
        "edit",
        "remove",
        "backup"
    ],
    "order": "!datetime",
    "model": {
        "label": "{{login}} (#{{id}})"
    },
    "buttons_edit": [
        {
            "type": "page",
            "page": "cms_users_logs?s_session={{id}}",
            "label_PL": "Logi sesji",
            "icon": "bar-chart"
        }
    ],
    "fields": [
        {
            "field": "datetime",
            "type": "datetime",
            "settings":{
                "default": "{{now}}"
            },
            "cms": {
                "label_PL": "Data i godzina",
                "label_EN": "Date",
                "default": "{{now}}",
                "list": "show",
                "search": true,
                "edit": false
            }
        },
        {
            "field": "login",
            "cms": {
                "label": "Login",
                "list": "show",
                "search": true,
                "edit": false
            }
        },
        {
            "field": "ip",
            "cms": {
                "label": "IP",
                "list": "show",
                "search": true,
                "edit": false
            }
        },
        {
            "field": "success",
            "type": "boolean",
            "cms": {
                "label_PL": "Poprawne",
                "label_EN": "Success",
                "list": "show",                
                "search": true,
                "edit": false
            }
        }
    ]
}