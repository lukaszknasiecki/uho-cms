{
    "table": "cms_users_logs_logins",
    "disable": [
        "add",
        "edit",
        "remove",
        "backup"
    ],
    "order": "!datetime",
    "model": {
        "label": "{{login}} (#{{id}})"
    },
    "buttons_edit": [
        {
            "type": "page",
            "page": "cms_users_logs?s_session={{id}}",
            "label_PL": "Logi sesji",
            "icon": "bar-chart"
        }
    ],
    "fields": [
        {
            "label_PL": "Data i godzina",
            "label_EN": "Date",
            "field": "datetime",
            "type": "datetime",
            "default":"{{now}}",
            "list":"show",
            "search": true,
            "edit": false
        },
        {
            "label": "Login",
            "field": "login",
            "list":"show",
            "search": true,
            "edit": false
        },
        {
            "label": "IP",
            "field": "ip",
            "list":"show",
            "search": true,
            "edit": false
        },
        {
            "label_PL": "Poprawne",
            "label_EN": "Success",
            "field": "success",
            "list":"show",
            "type": "boolean",
            "search": true,
            "edit": false
        }
    ]
}