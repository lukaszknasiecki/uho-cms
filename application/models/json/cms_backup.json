{
    "table": "cms_backup",
    "order": "!date",
    "label_PL": "Przywracane danych",
    "label_EN": "Backup restore",
    "disable": [
        "add",
        "remove",
        "backup"
    ],
    "buttons_edit": [
        {
            "label_PL": "Przywróć rekord",
            "label_EN": "Restore record",
            "icon": "restore",
            "plugin": "cms_backup_restore"
        }
    ],
    "buttons_page": [
        {
            "label": "Backup Media",
            "icon": "backup",
            "type": "page",
            "page": "cms_backup_media?s_page={{get.s_page}}&s_record={{get.s_record}}"
        }
    ],
    "fields": [
        {
            "label": "Data",
            "field": "date",
            "type": "date",
            "list": true,
            "edit": false
        },
        {
            "label": "Sesja",
            "field": "session",
            "list": true,
            "edit": false,
            "type": "select",
            "source": {
                "model": "cms_users_logs_logins"
            }
        },
        {
            "label_PL": "Strona",
            "label_EN": "Page",
            "field": "page",
            "list": true,
            "edit": false,
            "search": "strict"
        },
        {
            "label": "Rekord",
            "field": "record",
            "list": true,
            "edit": false,
            "search": true
        },
        {
            "label": "Dane",
            "field": "data",
            "edit": false,
            "type": "json"
        }
    ]
}