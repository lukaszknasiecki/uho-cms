{
    "table": "cms_backup",
    "order": "!date",
    "label": "Backup restore",
    "disable": [
        "add",
        "remove",
        "backup"
    ],
    "buttons_edit": [
        {
            "label_PL": "Przywróć rekord",
            "label_EN": "Restore record",
            "icon": "restore",
            "plugin": "cms_backup_restore"
        }
    ],
    "buttons_page": [
        {
            "label": "Backup Media",
            "icon": "backup",
            "type": "page",
            "page": "cms_backup_media?s_page={{get.s_page}}&s_record={{get.s_record}}"
        }
    ],
    "fields": [
        {
            "field": "date",
            "type": "date",
            "cms": {
                "label": "Data",
                "list": "show",
                "edit": false
            }
        },
        {
            "field": "session",
            "cms": {
                "label": "Sesja",
                "list": "show",
                "edit": false
            },
            "type": "select",
            "source": {
                "model": "cms_users_logs_logins"
            }
        },
        {
            "field": "page",
            "cms": {
                "label_PL": "Strona",
                "label_EN": "Page",
                "list": "show",
                "edit": false,
                "search": "strict"
            }
        },
        {
            "field": "record",
            "cms": {
                "label": "Rekord",
                "list": "show",
                "edit": false,
                "search": true
            }
        },
        {
            "field": "data",
            "type": "json",
            "cms": {
                "label": "Dane",
                "edit": false                
            }
        }
    ]
}