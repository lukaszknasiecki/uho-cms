{
    "label": "Worker Queue",
    
    "table": "uho_worker",
    "disable": [
        "add",
        "remove"
    ],
    "order": "date_created",
    "buttons_page": [
        {
            "plugin": "uho_worker_clear"
        },
        {
            "plugin": "uho_worker_errors"
        }
    ],
    "fields": [
        {
            "field": "id",
            "cms": {
                "label": "Id",
                "list": "show",
                "edit": false
            }
        },
        {
            "field": "date_created",
            "type": "datetime",
            "cms": {
                "label": "Created",
                "list": "show"
            }
        },
        {
            "field": "date_completed",
            "type": "datetime",
            "cms": {
                "label": "Processed",
                "list": "show",
                "edit": false
            }
        },
        {
            "field": "action",
            "cms": {
                "label": "Action",
                "list": "show",
                "edit": false
            }
        },
        {
            "field": "status",
            "type": "select",
            "options": [
                {
                    "value": "waiting",
                    "label_PL": "oczekująca",
                    "label_EN": "waiting"
                },
                {
                    "value": "success",
                    "label_PL": "zakończona",
                    "label_EN": "completed"
                },
                {
                    "value": "error",
                    "label_PL": "błędna",
                    "label_EN": "error"
                },
                {
                    "value": "processing",
                    "label_PL": "procesowana",
                    "label_EN": "processing"
                },
                {
                    "value": "disabled",
                    "label_PL": "dezaktywowana",
                    "label_EN": "disabled"
                }
            ],
            "cms": {
                "label": "Status",
                "list": true,
                "edit": false,
                "search": true
            }
        },
        {
            "field": "message",
            "type": "text",
            "cms": {
                "label": "Message",
                "list": "show",
                "edit": false
            }
        }
    ]
}